{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.alfa.cockpit",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "dataSources": {
      "mainService": {
        "uri": "/odata/v2/Catalog/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {}
      }
    },
    "models": {
      "i18n": { "type": "sap.ui.model.resource.ResourceModel", "settings": { "bundleName": "com.alfa.cockpit.i18n.i18n" } },
      "": { "dataSource": "mainService", "preload": true, "settings": {} }
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.alfa.cockpit.view",
        "controlId": "navContainer",
        "controlAggregation": "pages",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "overview",
          "pattern": "",
          "target": "overview"
        },
        {
          "name": "users",
          "pattern": "users",
          "target": "users"
        }
      ],
      "targets": {
        "overview": {
          "viewName": "Overview",
          "viewLevel": 1
        },
        "users": {
          "viewName": "Users",
          "viewLevel": 2
        }
      }
    },
    "rootView": {
      "viewName": "com.alfa.cockpit.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}