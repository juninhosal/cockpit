{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.alfa.cockpit",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.0",
      "toolsId": "fe3aefcc-2ed4-4b78-89d3-1159c346f691"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.141.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.alfa.cockpit.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "dataSources": {
        "mainService": {
          "uri": "/odata/v2/Catalog/",
          "type": "OData",
          "settings": {
            "odataVersion": "2.0"
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.alfa.cockpit.view",
        "controlId": "navContainer",
        "controlAggregation": "pages",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "overview",
          "pattern": "",
          "target": [
            "overview"
          ]
        },
        {
          "name": "users",
          "pattern": "users",
          "target": [
            "usersList"
          ]
        },
        {
          "name": "userDetail",
          "pattern": "users/{userId}",
          "target": [
            "usersList",
            "userDetail"
          ]
        },
        {
          "name": "roleDetail",
          "pattern": "users/{userId}/role/{roleCollectionId}",
          "target": [
            "usersList",
            "userDetail",
            "roleDetail"
          ]
        }
      ],
      "targets": {
        "overview": {
          "viewName": "Overview",
          "viewId": "overview",
          "viewLevel": 1
        },
        "users": {
          "viewName": "Users",
          "viewId": "users",
          "viewLevel": 2
        },
        "usersList": {
          "parent": "users",
          "viewName": "UsersList",
          "controlId": "fcl",
          "controlAggregation": "beginColumnPages"
        },
        "userDetail": {
          "parent": "users",
          "viewName": "UserDetail",
          "controlId": "fcl",
          "controlAggregation": "midColumnPages"
        },
        "roleDetail": {
          "parent": "users",
          "viewName": "RoleDetail",
          "controlId": "fcl",
          "controlAggregation": "endColumnPages"
        }
      }
    },
    "rootView": {
      "viewName": "com.alfa.cockpit.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}